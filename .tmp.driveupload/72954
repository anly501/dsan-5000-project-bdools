  <us-gaap:OtherNonoperatingIncomeExpense contextRef="eol_PE6794----1110-K0010_STD_365_20111231_0" unitRef="iso4217_USD" decimals="0">-12682</us-gaap:OtherNonoperatingIncomeExpense>
