    <TD STYLE="width: 1%; font-size: 10pt; text-align: left">$</TD><TD STYLE="width: 10%; font-size: 10pt; text-align: right">6,156,191</TD><TD STYLE="width: 1%; font-size: 10pt; text-align: left">&nbsp;</TD><TD STYLE="width: 2%; font-size: 10pt">&nbsp;</TD>
    <TD STYLE="width: 1%; font-size: 10pt; text-align: left">&nbsp;</TD><TD STYLE="width: 10%; font-size: 10pt; text-align: right">10.9</TD><TD STYLE="width: 1%; font-size: 10pt; text-align: left">%</TD></TR>
<TR STYLE="vertical-align: bottom; background-color: White">
    <TD STYLE="font-size: 10pt; text-align: justify; padding-bottom: 1pt">Cost of sales</TD><TD STYLE="font-size: 10pt; padding-bottom: 1pt">&nbsp;</TD>
    <TD STYLE="border-bottom: Black 1pt solid; font-size: 10pt; text-align: left">&nbsp;</TD><TD STYLE="border-bottom: Black 1pt solid; font-size: 10pt; text-align: right">44,218,300</TD><TD STYLE="padding-bottom: 1pt; font-size: 10pt; text-align: left">&nbsp;</TD><TD STYLE="font-size: 10pt; padding-bottom: 1pt">&nbsp;</TD>
    <TD STYLE="border-bottom: Black 1pt solid; font-size: 10pt; text-align: left">&nbsp;</TD><TD STYLE="border-bottom: Black 1pt solid; font-size: 10pt; text-align: right">39,946,724</TD><TD STYLE="padding-bottom: 1pt; font-size: 10pt; text-align: left">&nbsp;</TD><TD STYLE="font-size: 10pt; padding-bottom: 1pt">&nbsp;</TD>
    <TD STYLE="border-bottom: Black 1pt solid; font-size: 10pt; text-align: left">&nbsp;</TD><TD STYLE="border-bottom: Black 1pt solid; font-size: 10pt; text-align: right">4,271,576</TD><TD STYLE="padding-bottom: 1pt; font-size: 10pt; text-align: left">&nbsp;</TD><TD STYLE="font-size: 10pt; padding-bottom: 1pt">&nbsp;</TD>
    <TD STYLE="font-size: 10pt; text-align: left">&nbsp;</TD><TD STYLE="font-size: 10pt; text-align: right">10.7</TD><TD STYLE="padding-bottom: 1pt; font-size: 10pt; text-align: left">%</TD></TR>
